//Swap Using XOR


#include<iostream>
using namespace std;

int main(){
    int a,b;
    cout<<"Enter First Integer:";
    cin>>a;
    cout<<"Enter second Integer:";
    cin>>b;
    
    cout<<"Before Swaping:\n";
    cout<<"A="<<a<<endl;
    cout<<"B="<<b<<endl;
    
    
    a=a^b;
    b=a^b;
    a=a^b;
    
    cout<<"After Swaping:\n";
    cout<<"A="<<a<<endl;
    cout<<"B="<<b<<endl;

}
